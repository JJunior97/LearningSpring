<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean id = "knife" class="Rozdzial3.Inject.Knife" scope="prototype"/>

    <bean id = "saxophone" class="Rozdzial3.Inject.Saxophone"/>
    <bean id = "piano" class="Rozdzial3.Inject.Piano"/>
    <bean id = "guitar" class="Rozdzial3.Inject.Guitar"/>
    <bean id = "hammeredDulcimer" class="Rozdzial3.Inject.HammeredDulcimer"/>

    <bean id = "gorn" class="Rozdzial3.Inject.KnifeJuggler"/>

    <bean id = "duke" class="Rozdzial3.Inject.Instrumentalist"/>




</beans>